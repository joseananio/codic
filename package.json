{
  "name": "codic",
  "version": "2.1.0",
  "description": "Easy dynamic nodejs, javascript job scheduler for any database",
  "author": {
    "name": "Joseph Anane",
    "url": "http://github.com/joseananio"
  },
  "license": "MIT",
  "homepage": "http://github.com/joseananio",
  "main": "dist/cjs/bundle.js",
  "module": "dist/es/bundle.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "types": "dist/cjs/lib/codic/index.d.ts",
  "directories": {
    "test": "test",
    "lib": "lib"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:es": "NODE_ENV=es rollup -c",
    "build:cjs": "NODE_ENV=cjs rollup -c",
    "build:umd": "NODE_ENV=umd rollup -c",
    "build": "run-s clean && run-p build:cjs build:es",
    "test": "NODE_ENV=test mocha --recursive -r ts-node/register test/**/*.test.ts"
  },
  "dependencies": {
    "human-interval": "^0.1.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/es6-promise": "^3.3.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.11",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-typescript2": "^0.18.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.1"
  },
  "keywords": [
    "job",
    "task",
    "scheduler",
    "jobs",
    "job",
    "runner",
    "cron",
    "typescript",
    "js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/joseananio/codic"
  },
  "bugs": {
    "url": "https://github.com/joseananio/codic/issues"
  }
}
